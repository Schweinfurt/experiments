<?xml version="1.0" encoding="utf-8"?>
<!-- Launch file -->

<launch>
    <!-- Defining the node and executable and publishing the output on terminal-->
    <node pkg="experiments" name="twist_bolt_control" type="twist_bolt_control.py" output="screen">
        <!-- loading the parameters from yaml file during th runtime -->
        <rosparam command="load" file="$(find experiments)/config/experiments.yaml"/>
    </node>
</launch>
